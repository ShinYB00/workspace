package ex02.api;

public class MainEntry {
	public static void main(String[] args) {
		
		String s1 = new String("Korea");
		String s2 = new String("Korea");
		
		int x = 3, y = 3;
		
		String s3 = "Korea";
		String s4 = "Korea";
		
		if (s1==s2) {
			System.out.println("s1==s2");
		} else {
			System.out.println("s1!=s2");
		}
		
		System.out.println("====================");
		
		if (x==y) {
			System.out.println("x==y");
		} else {
			System.out.println("x!=y");
		}
		
		System.out.println("====================");
		
		if (s3==s4) {
			System.out.println("s3==s4");
		} else {
			System.out.println("s3!=s4");
		}
		
//		String str = "abc";
